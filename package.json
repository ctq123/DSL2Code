    "@monaco-editor/react": "^4.1.3",
    "@stackblitz/sdk": "^1.5.2",
    "@types/gtag.js": "^0.0.7",
    "@umijs/preset-react": "1.x",
    "antd": "^4.12.3",
    "copy-to-clipboard": "^3.3.1",
    "file-saver": "^2.0.5",
    "jszip": "^3.6.0",
    "lodash": "^4.17.21",
    "puppeteer": "^10.0.0",
    "serialize-javascript": "^5.0.1",
    "umi": "^3.3.9",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/test": "^3.3.9",
    "@welldone-software/why-did-you-render": "^6.2.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "react": "17.x",
    "react-dom": "17.x",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
